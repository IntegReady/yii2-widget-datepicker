{
  "name": "integready/yii2-datepicker-widget",
  "description": "Enhanced Yii2 wrapper for the bootstrap datepicker plugin (sub repo split from yii2-widgets).",
  "keywords": [
    "yii2",
    "extension",
    "widget",
    "select2",
    "form",
    "date",
    "picker",
    "jquery",
    "plugin"
  ],
  "homepage": "https://github.com/IntegReady/yii2-widget-datepicker",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "IntegReady"
    }
  ],
  "require": {
    "kartik-v/yii2-krajee-base": "~1.7",
    "kartik-v/yii2-field-range": "*"
  },
  "require-dev": {
    "yiisoft/yii2-coding-standards": "*"
  },
  "autoload": {
    "psr-4": {
      "integready\\datepicker\\": ""
    }
  }
}